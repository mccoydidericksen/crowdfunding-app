
{{#each projects}}
<div class="column mb-5">
  <div class="col-md-6">
    <h2><a href="/project/{{id}}">Project: {{name}}</a></h2>
  </div>
  <div class="col-md-6">
    <h3>Description:</h3>
    <p>{{description}}</p>
  </div>
  <div class="col-md-6">
    <h3>Funding Needed: ${{needed_funding}}</h3>
  </div>
</div>
{{/each}}
